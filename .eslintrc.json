{
  "root": true,
  "parser": "vue-eslint-parser",
  "extends": [
    "eslint:recommended",
    "plugin:vue/vue3-recommended",
    "plugin:json/recommended-with-comments",
    "plugin:markdown/recommended",
    "@nuxtjs/eslint-config-typescript",
    "prettier"
  ],
  "rules": {
    "vue/multi-word-component-names": [
      "error",
      {
        "ignores": ["index"]
      }
    ]
  }
}
